{
  "extends": [
    "eslint:recommended",
    "plugin:mdx/recommended",
    "plugin:prettier/recommended"
  ],
  // optional, if you want to lint code blocks at the same time
  "settings": {
    "mdx/code-blocks": true
  },
  "rules": {
    "no-undef": "off",
    "no-unused-expressions": "off",
    "no-unused-vars": "off"
  }
}
